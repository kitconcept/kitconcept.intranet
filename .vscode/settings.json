{
  "eslint.workingDirectories": ["./frontend"],
  "flake8.cwd": "${workspaceFolder}/backend",
  "flake8.args": [
    "--config=pyproject.toml"
  ],
  "black-formatter.cwd": "${workspaceFolder}/backend",
  "isort.args": ["--settings-path=${workspaceFolder}/backend/pyproject.toml"],
  "ansible.python.interpreterPath": "./devops/bin/python",
  "ansible.validation.lint.path": "./devops/bin/ansible-lint",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#314158",
    "activityBar.background": "#314158",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#000000",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#314158",
    "statusBar.background": "#1f2937",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#314158",
    "statusBarItem.remoteBackground": "#1f2937",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1f2937",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1f293799",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1f2937"
}
