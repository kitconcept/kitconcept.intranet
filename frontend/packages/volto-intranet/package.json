  },
  "devDependencies": {
    "@plone/scripts": "^3.6.2",
    "@plone/types": "workspace:*",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@testing-library/react": "^16.2.0",
    "release-it": "^17.1.1",
    "typescript": "^5.7.3",
    "vitest": "^3.1.2"
  },
  "addons": [
    "volto-form-block",
    "@kitconcept/core",
    "@kitconcept/volto-solr"
  ],
  "dependencies": {
    "@plone/components": "workspace:*",
