  "description": "A Plone distribution for Intranets with Plone. Created by kitconcept.",
  "main": "src/index.ts",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "kitconcept GmbH",
